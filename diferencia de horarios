


#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main () 
{
int h,h1,h2,m,m1,m2,s,s1,s2,x,y,z;
cout <<"Ingrese el primer horario" << endl;
cin>>h1>>m1>>s1;
cout<<"Ingrese el segundo horario" << endl;
cin>>h2>>m2>>s2;
x=(h1*3600)+m1*60+s1;
y=(h2*3600)+m2*60+s2;
if(x>y){
	z=x-y;
}
else {
	if (y>x) {
	z=y-x;
	}
	else
	{
	z=0;
	}
}
h=z/3600;
m=(z/60)-(h*60);
s=z-(m*60)-(h*3600);
cout<<"Tiempo transcurrido : "<<h<<":"<<m<<":"<<s<<endl;
getch ();
}
