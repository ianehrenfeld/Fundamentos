// area de un rectangulo.cpp : Defines the entry point for the console application.
//
#include "stdafx.h"
#include<iostream>
#include "conio.h"

using namespace std;


int main() {
	float altura;
	float area;
	float base;
	cout << "introduzca la altura " << endl;
	cin >> altura;
	cout << "introduzca base" << endl;
	cin >> base;
	area = altura*base;
	cout << "el area es" << endl;
	cout << area << endl;
	getch();
	return 0;
}

